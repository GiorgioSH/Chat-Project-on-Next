s(data);\n    });\n    return (\n        <StyledCard>\n            <label htmlFor=\"username\">\n                Enter your name\n                <Input\n                    name=\"username\"\n                    placeholder=\"Enter your username\"\n                    maxLength={25}\n                    value={username}\n                    onChange={onUsernameChange}\n                />\n            </label>\n            <label htmlFor=\"room\">\n                Enter room number of your choice\n                <Input\n                    name=\"room\"\n                    placeholder=\"Enter your room number\"\n                    maxLength={25}\n                    value={room}\n                    onChange={onRoomChange}\n                />\n            </label>\n            <StyledButton \n                type=\"primary\" \n                size={\"large\"}\n                onClick={onClick}\n            >\n                Join the Chat Room\n            </StyledButton>\n        </StyledCard>\n    )\n};\n\nexport default JoinRoom;\n\nconst StyledCard = styled(Card)`\n    width: 581px;\n    height: 210px;\n    margin: 30vh auto;\n    box-shadow: 2px 3px 3px 2.8px #d7d7e4;\n    text-align: center;\n`\nconst StyledButton = styled(Button)`\n    margin-top: 10px;\n`\n","/Users/admin/Documents/GitHub/Chat-Project-on-Next/my-project/strapi-blog-ui-starter/src/screens/ChatRoom/index.js",["59"],"/Users/admin/Documents/GitHub/Chat-Project-on-Next/my-project/strapi-blog-ui-starter/src/screens/ChatRoom/styles.js",[],"/Users/admin/Documents/GitHub/Chat-Project-on-Next/my-project/strapi-blog-ui-starter/src/config/web-sockets.js",[],"/Users/admin/Documents/GitHub/Chat-Project-on-Next/my-project/strapi-blog-ui-starter/src/components/Messages/index.js",[],"/Users/admin/Documents/GitHub/Chat-Project-on-Next/my-project/strapi-blog-ui-starter/src/components/Header/index.js",[],"/Users/admin/Documents/GitHub/Chat-Project-on-Next/my-project/strapi-blog-ui-starter/src/components/Mess